  "query_meta": [
    {
      "tit": "mainq",
      "rep": "Geo And  Area Pending",
      "sq": "SELECT t2.sowtran1id, a6.ragid, CONCAT(a6.ragid,' - ',UPPER(a5.npartyname)) farmerid, 'viewreport' AS viewreportbutton\r\nFROM sowtran1 t2, frmmast a5, cropcycle a6, cropmaster a9, igdetail a10, empmaster a11\r\nWHERE t2.farmername=a5.frmmastid AND t2.sowtran1id = a6.upstream_txview_ref_id \r\nAND a6.cropmasteruid = a9.cropmasterid AND a9.crop = a10.igdetailid \r\nAND a6.fieldstaff = a11.empmasterid AND (a6.ragid NOT IN (SELECT t1.ragid FROM areaaudittag t1) OR\r\na6.ragid NOT IN (SELECT t1.ragid FROM farmgeotagging t1))\r\nAND a10.subgroupid = :crop AND a6.cycleclosedn=0 \r\nAND a11.mobno = :mobno AND a6.sowdt  >= DATE_FORMAT(STR_TO_DATE(:sdate,'%d-%b-%y'),'%Y-%m-%d')\r\nORDER BY 2",
      "imq": "T",
      "jot": "none",
      "isq": "F"
    }
