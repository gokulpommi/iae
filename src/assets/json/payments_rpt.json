    "query_meta": [
        {
            "tit": "Payments",
            "rep": "Payments",
            "sq": "SELECT a.docid AS capno, b.paytype AS capdesc, \t\t\r\nb.chqno AS capchqno, a.chqdt AS capchqdt, m.mname, \t\t\r\nb.amtpaid AS capamt  \t\t\r\nFROM frmpay2 b, frmpay1 a, cropcycle c, master m\t\t\r\nWHERE a.frmpay1id = b.frmpay1id\t\t\r\nAND c.cropcycleid = b.cropcycleid AND a.bankname = m.masterid\t\t\r\nAND c.ragid = :ragid\r\nUNION\r\nSELECT a.docid AS capno, 'Payment Return' AS capdesc, \t\t\r\nb.chqno AS capchqno, a.trndate AS capchqdt, m.mname, \t\t\r\n-(b.amtrecd) AS capamt  \t\t\r\nFROM payretdetail b, payretbasic a, cropcycle c, master m\t\t\r\nWHERE a.payretbasicid = b.payretbasicid\t\t\r\nAND c.cropcycleid = b.updcropcycleid AND a.bankacc = m.masterid\t\t\r\nAND c.ragid = :ragid\r\nORDER BY 4",
            "imq": "T",
            "jot": "none",
            "isq": "F"
        }
