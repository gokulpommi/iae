    "query_meta": [
        {
            "tit": "FRM Analysis Manager",
            "rep": "Farmer Analysis Manager",
            "sq": "SELECT CONCAT(a1.ragid,' - ',a1.farmerid) AS farmername, a1.sowdt, a1.village, a1.acres, a1.crop, SUM(a1.OpBal) OpBal, SUM(a1.CROPVAL) CROPVAL, SUM(a1.BONUS_AMT) BONUS_AMT, SUM(a1.INPUTVAL) INPUTVAL, SUM(a1.AMTPAID) AMTPAID, SUM(a1.ADJAMT) ADJAMT, SUM(a1.CL_BAL) CL_BAL,\r\nMAX(a1.EDOH) EDOH, MAX(a1.LDOH) LDOH\r\nFROM (\r\nSELECT a1.region, a2.branchname AS branchid, a3.areaname AS AREA, a8.cspuid AS Month_Plan, a9.cropscheme, a10.subgroupid AS crop, \r\na6.ragid, a6.agrno, a6.frmcropcycle, a5.npartyname AS farmerid, a7.villagename AS village, a11.employee_name AS FSName, a6.sowdt, a6.Sweekno AS weekno, a6.acressown AS acres,\r\n0 AS OpBal, \r\n(SUM(CASE t1.mapname WHEN 'CPSAD1' THEN t1.debit ELSE 0 END)*-1)  AS CROPVAL, 0 AS BONUS_AMT, \r\nSUM(CASE SUBSTR(t1.mapname,1,3)WHEN 'inv' THEN t1.debit ELSE 0 END) +  \r\nSUM(CASE t1.mapname WHEN 'ISCMR4' THEN t1.debit ELSE 0 END) AS INPUTVAL, \r\nSUM(CASE t1.mapname WHEN 'sainv1' THEN t1.debit ELSE 0 END) AS AMTPAID, \r\nSUM(CASE t1.mapname WHEN 'ftran1' THEN t1.debit ELSE 0 END) + \r\nSUM(CASE t1.mapname WHEN 'acvch1' THEN t1.debit ELSE 0 END) + \r\nSUM(CASE t1.mapname WHEN 'finse1' THEN t1.debit ELSE 0 END) + \r\nSUM(CASE t1.mapname WHEN 'finse3' THEN t1.debit ELSE 0 END) + \r\nSUM(CASE t1.mapname WHEN 'finse4' THEN t1.debit ELSE 0 END) AS ADJAMT, SUM(t1.debit*-1) AS CL_BAL, a6.cropcycleid, \r\nADDDATE(a6.sowdt,a6.hardays) AS EDOH, NULL AS LDOH \r\nFROM frmtrans t1, cropcycle a6, regiondetails a1, branchmast a2, amaster a3,  frmmast a5, vmaster a7, csp1 a8, cropmaster a9, igdetail a10, empmaster a11\r\nWHERE t1.cropcycleid = a6.cropcycleid AND a1.regiondetailsid=a2.region AND a6.branchid=a2.branchmastid AND a6.areaname=a3.amasterid \r\nAND a6.farmerid=a5.frmmastid  AND a6.village = a7.vmasterid AND a6.cspuid = a8.csp1id AND a8.cropmasteruid = a9.cropmasterid \r\nAND a8.Crop = a10.IGDetailid AND a6.fieldstaff = a11.empmasterid AND a6.cycleclosedn=0 AND a6.cycleclosed='NO'\r\nAND a11.mobno = :mobno \r\nAND a11.employee_name = :fsname \r\nGROUP BY a1.region, a2.branchname, a3.areaname, a11.employee_name, a8.cspuid, a9.cropscheme, a10.subgroupid, a6.ragid, a6.agrno, a6.frmcropcycle, \r\na5.npartyname, a7.villagename, a11.employee_name, a6.sowdt, a6.Sweekno, a6.acressown, a6.cropcycleid, ADDDATE(a6.sowdt,a6.hardays)\r\nUNION ALL\r\nSELECT a1.region, a2.branchname AS branchid, a3.areaname AS AREA, a8.cspuid AS Month_Plan, a9.cropscheme, a10.subgroupid AS crop, \r\na6.ragid, a6.agrno, a6.frmcropcycle, a5.npartyname AS farmerid, a7.villagename AS village, a11.employee_name AS FSName, a6.sowdt, a6.Sweekno AS weekno, a6.acressown AS acres,\r\n0 AS OpBal, \r\nSUM(CASE t2.mapname WHEN 'cpsgr1' THEN t2.credit ELSE 0 END) + \r\nSUM(CASE t2.mapname WHEN 'cpsad3' THEN t2.credit ELSE 0 END) AS CROPVAL, 0 AS BONUS_AMT, \r\nSUM(CASE t2.mapname WHEN 'SalRt2' THEN (t2.credit*-1) ELSE 0 END) AS INPUTVAL, \r\nSUM(CASE t2.mapname WHEN 'payrt1' THEN (t2.credit*-1) ELSE 0 END) AS AMTPAID, \r\nSUM(CASE t2.mapname WHEN 'ftran2' THEN (t2.credit) ELSE 0 END*-1) + \r\nSUM(CASE t2.mapname WHEN 'acvch2' THEN (t2.credit) ELSE 0 END*-1) + \r\nSUM(CASE t2.mapname WHEN 'finse2' THEN t2.credit ELSE 0 END*-1) +\r\nSUM(CASE t2.mapname WHEN 'finse5' THEN t2.credit ELSE 0 END*-1) AS ADJAMT, \r\nSUM(t2.credit) AS CL_BAL, a6.cropcycleid, ADDDATE(a6.sowdt,a6.hardays) AS EDOH, \r\n(SELECT MAX(z.docdate) FROM frmtrans2 z WHERE z.mapname='cpsgr1' AND z.cropcycleid=a6.cropcycleid) AS LDOH \r\nFROM frmtrans2 t2, cropcycle a6, regiondetails a1, branchmast a2, amaster a3,  frmmast a5, vmaster a7, csp1 a8, cropmaster a9, igdetail a10, empmaster a11\r\nWHERE t2.cropcycleid = a6.cropcycleid AND a1.regiondetailsid=a2.region AND a6.branchid=a2.branchmastid AND a6.areaname=a3.amasterid \r\nAND a6.farmerid=a5.frmmastid  AND a6.village = a7.vmasterid AND a6.cspuid = a8.csp1id AND a8.cropmasteruid = a9.cropmasterid \r\nAND a8.Crop = a10.igdetailid AND a6.fieldstaff = a11.empmasterid AND a6.cycleclosedn=0 AND a6.cycleclosed='NO'\r\nAND a11.mobno = :mobno\r\nAND a11.employee_name = :fsname\r\nGROUP BY a1.region, a2.branchname, a3.areaname, a11.employee_name, a8.cspuid, a9.cropscheme, a10.subgroupid, a6.ragid, a6.agrno, a6.frmcropcycle, \r\na5.npartyname, a7.villagename, a11.employee_name, a6.sowdt, a6.Sweekno, a6.acressown,a6.cropcycleid, (a6.sowdt+a6.hardays)\r\nORDER BY 2,1) a1\r\nGROUP BY a1.region, a1.branchid, a1.area, a1.Month_Plan, a1.cropscheme, a1.crop, a1.ragid, a1.agrno, a1.frmcropcycle, a1.farmerid, a1.village, a1.FSName, a1.sowdt, a1.weekno, a1.acres, a1.cropcycleid, a1.EDOH, a1.ragid\r\nORDER BY 2,1",
            "imq": "T",
            "jot": "none",
            "isq": "F"
        }
    ],
