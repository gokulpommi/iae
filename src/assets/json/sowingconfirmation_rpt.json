     "query_meta": [
        {
            "tit": "Main",
            "rep": "Sowing Confirmation",
            "sq": "SELECT f.frmbooking1id, b.branchname,b.branchmastid, a.areaname,a.amasterid, v.villagename,v.vmasterid, f.farmername, f.fathername, f.mobno, f.irgtype, \r\nf.cropname, f.cropscheme, f.acres, f.totissqty issqty, f.expsowdt, f.fstatus\r\nFROM frmbooking1 f,locdetails l,amaster a,branchmast b,empmaster e, vmaster v\r\nWHERE f.empname=e.empmasterid AND f.areaname=a.amasterid AND f.branchid=b.branchmastid AND f.village=v.vmasterid\r\nAND f.fslocid=l.locdetailsid AND e.mobno=:mobno AND a.areaname=:areaname\r\nAND f.fstatus='YES' \r\nUNION ALL\r\nSELECT f.frmbooking1id, b.branchname,b.branchmastid, a.areaname,a.amasterid, v.villagename,v.vmasterid, f.farmername, f.fathername, f.mobno, f.irgtype, \r\nf.cropname, f.cropscheme, f.acres, f.totissqty issqty, f.expsowdt, f.fstatus\r\nFROM frmbooking1 f,locdetails l,amaster a,branchmast b,empmaster e, vmaster v, empmaster e1\r\nWHERE f.empname=e.empmasterid AND f.areaname=a.amasterid AND f.branchid=b.branchmastid AND f.village=v.vmasterid\r\nAND f.fslocid=l.locdetailsid AND e.areamanager = e1.employee_name\r\nAND e1.mobno=:mobno AND a.areaname=:areaname\r\nAND f.fstatus='YES' \r\nORDER BY 16",
            "imq": "T",
            "jot": "none",
            "isq": "T"
        }
    ],
