  "query_meta": [
    {
      "tit": "List of Available Framer",
      "rep": "Farmer APP",
      "sq": "SELECT CONCAT (a6.ragid,' - ',a5.npartyname) AS farmerid, a6.ragid, a5.npartyname,'viewreport' AS viewreportbutton\r\nFROM sowtran1 t1, regiondetails a1, branchmast a2, amaster a3, frmmast a5, cropcycle a6, vmaster a7, csp1 a8, cropmaster a9,\r\nigdetail a10, empmaster a11\r\nWHERE a1.regiondetailsid=a2.region AND t1.branchid=a2.branchmastid AND t1.areaname=a3.amasterid AND t1.farmername=a5.frmmastid\r\nAND t1.sowtran1id = a6.upstream_txview_ref_id AND a6.village = a7.vmasterid AND a6.cspuid = a8.csp1id\r\nAND a8.cropmasteruid = a9.cropmasterid AND a8.crop = a10.igdetailid AND a6.fieldstaff = a11.empmasterid \r\nAND a10.subgroupid = :crop AND a11.mobno =:mobno AND a6.sowdt  BETWEEN DATE_FORMAT (STR_TO_DATE(:sdate,'%d-%b-%y'),'%Y-%m-%d') AND DATE_FORMAT (STR_TO_DATE(:edate,'%d-%b-%y'),'%Y-%m-%d')\r\nUNION ALL\r\nSELECT CONCAT (a6.ragid,' - ',a5.npartyname) AS farmerid, a6.ragid, a5.npartyname,'viewreport' AS viewreportbutton\r\nFROM sowtran1 t1, regiondetails a1, branchmast a2, amaster a3, frmmast a5, cropcycle a6, vmaster a7, csp1 a8, cropmaster a9,\r\nigdetail a10, empmaster a11\r\nWHERE a1.regiondetailsid=a2.region AND t1.branchid=a2.branchmastid AND t1.areaname=a3.amasterid AND t1.farmername=a5.frmmastid\r\nAND t1.sowtran1id = a6.upstream_txview_ref_id AND a6.village = a7.vmasterid AND a6.cspuid = a8.csp1id\r\nAND a8.cropmasteruid = a9.cropmasterid AND a8.crop = a10.igdetailid AND a8.areamanager = a11.empmasterid\r\nAND a10.subgroupid = :crop  AND a11.mobno = :mobno\r\nAND a6.sowdt  BETWEEN DATE_FORMAT (STR_TO_DATE(:sdate,'%d-%b-%y'),'%Y-%m-%d')\r\nAND DATE_FORMAT (STR_TO_DATE(:edate,'%d-%b-%y'),'%Y-%m-%d')\r\nORDER BY ragid",
      "imq": "T",
      "jot": "none",
      "isq": "F"
    }
  ],
