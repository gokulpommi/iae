    "query_meta": [
        {
            "tit": "Input",
            "rep": "Inputs",
            "sq": "SELECT b.cmrno AS docno, b.cmrdt AS docdt,\r\n im.itemid AS description, b.qty AS qty,\r\n b.amount AS totamt,b.fvc,0 AS cropage\t\t\r\nFROM sowtran1 a, sowtran2 b, itemmaster im, igdetail ig\t\t\r\nWHERE a.sowtran1id=b.sowtran1id AND b.itemid = im.itemmasterid AND im.subgroup = ig.igdetailid \r\nAND a.sowtran1id IN (SELECT upstream_txview_ref_id FROM cropcycle cc WHERE cc.ragid = :ragid)\t\t\r\nUNION ALL\t\t\r\nSELECT  a.cmrno AS docno, a.cmrdt AS docdt, im.itemid AS description,  b.qty AS qty,\r\nb.amount AS totamt, \t\t\r\n b.fvc, (a.cmrdt-c.sowdt) AS cropage\t\t\r\nFROM cmr1 a, cmr2 b, itemmaster im, igdetail ig, cropcycle c\t\t\r\nWHERE a.cmr1id=b.cmr1id AND b.itemid = im.itemmasterid AND im.subgroup = ig.igdetailid AND a.cropcycleid = c.cropcycleid AND c.ragid  =  :ragid\t\t\r\nUNION ALL\t\t\r\nSELECT a.cirno AS docno, a.cirdt AS docdt,im.itemid AS description,  (b.qty*-1) AS qty, \r\n(b.amount*-1) AS totamt, \t\t\r\n b.fvc, a.cropage\t\t\r\nFROM cir1 a, cir2 b, itemmaster im, igdetail ig, cropcycle c\t\t\r\nWHERE a.cir1id=b.cir1id AND b.itemid = im.itemmasterid AND im.subgroup = ig.igdetailid AND a.cropcycleid = c.cropcycleid AND c.ragid  = :ragid\r\nORDER BY 2,1,3",
            "imq": "T",
            "jot": "none",
            "isq": "F"
        }
    ],
