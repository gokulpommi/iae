    "query_meta": [
        {
            "tit": "Main",
            "rep": "Attendance Report",
            "sq": "SELECT a.docdate, a.idcard, CONCAT(a.idcard,' ',a.docdate) login_id, e.employee_name, a.intime, \r\n(SELECT a1.outtime FROM attpunch a1, empmaster e1 WHERE a1.idcard=e1.mobno \r\nAND a1.docdate BETWEEN DATE_FORMAT (STR_TO_DATE(:sdate,'%d-%b-%y'),'%Y-%m-%d') AND DATE_FORMAT (STR_TO_DATE(:edate,'%d-%b-%y'),'%Y-%m-%d') \r\nAND a1.outtime IS NOT NULL AND CONCAT(a.idcard,' ',a.docdate) = CONCAT(a1.idcard,' ',a1.docdate)\r\n) outtime\r\nFROM attpunch a, empmaster e,emparea e1,amaster a1\r\nWHERE a.idcard=e.mobno AND a.docdate BETWEEN DATE_FORMAT (STR_TO_DATE(:sdate,'%d-%b-%y'),'%Y-%m-%d') \r\nAND DATE_FORMAT (STR_TO_DATE(:edate,'%d-%b-%y'),'%Y-%m-%d') AND a.intime IS NOT NULL \r\nand e.empmasterid=e1.empmasterid and e1.emparea1=a1.amasterid and a1.areaname=:areaname\r\nORDER BY 1,2",
            "imq": "T",
            "jot": "none",
            "isq": "F"
        }
