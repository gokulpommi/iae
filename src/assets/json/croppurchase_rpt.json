  "query_meta": [
    {
      "tit": "crop",
      "rep": "Crop Purchase",
      "sq": "SELECT cp1.cpsdt, cp1.cpsno, cp2.grd1qty,cp2.grd2qty,cp2.grd3qty,cp2.grd4qty,cp2.grd5qty,cp2.grd8qty AS g8qty, cp2.totalqty,cp2.totalamt, cm.cropscheme\t\t\r\nFROM cpsg1 cp1, cropcycle cc, cpsg2 cp2, cropmaster cm WHERE cp1.cpsg1id = cp2.cpsg1id AND  cc.ragid = :ragid AND cp2.ragrno = cc.cropcycleid AND cp2.cropscheme = cm.cropmasterid\t\t\r\nUNION ALL\t\t\r\nSELECT cp1.cprdt AS cpsdt, cp1.cprno AS cpsno, (cp2.grd1qty*-1) AS grd1qty, (cp2.grd2qty*-1) AS grd2qty, (cp2.grd3qty*-1) AS grd3qty, \t\t\r\n(cp2.grd4qty*-1) AS grd4qty, (cp2.grd5qty*-1) AS grd5qty,(cp2.grd8qty*-1) AS g8qty, \t\t\r\n (cp2.totalqty*-1) AS totalqty, (cp2.totalamt*-1) AS totalamt,cm.cropscheme\t\t\r\nFROM cpsad1 cp1, cropcycle cc, cpsad2 cp2, cropmaster cm WHERE cp1.cpsad1id = cp2.cpsad1id AND  cc.ragid = :ragid AND cp2.ragrno = cc.cropcycleid \t\t\r\nAND cp2.cropscheme = cm.cropmasterid AND cp2.adtype = 'LESS'\t\t\r\nUNION ALL\t\t\r\nSELECT cp1.cprdt AS cpsdt, cp1.cprno AS cpsno, cp2.grd1qty AS grd1qty, cp2.grd2qty AS grd2qty, cp2.grd3qty AS grd3qty, \t\t\r\ncp2.grd4qty AS grd4qty, \t\t\r\ncp2.grd5qty AS grd5qty,  cp2.grd8qty AS g8qty, cp2.totalqty AS totalqty, cp2.totalamt AS totalamt, cm.cropscheme\t\t\r\nFROM cpsad1 cp1, cropcycle cc, cpsad2 cp2, cropmaster cm WHERE cp1.cpsad1id = cp2.cpsad1id AND  cc.ragid = :ragid AND cp2.ragrno = cc.cropcycleid \t\t\r\nAND cp2.cropscheme = cm.cropmasterid AND cp2.adtype <> 'LESS'\t\t\r\nORDER BY 1",
      "imq": "T",
      "jot": "none",
      "isq": "F"
    }
  ],
